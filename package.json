{
  "name": "vscode-diff",
  "displayName": "Diff",
  "description": "Diff 2 opened files with ease. Because running `code --diff path1 path2` is too slow.",
  "icon": "resources/logo-128x128.png",
  "version": "1.3.0",
  "license": "MIT",
  "main": "out/src/extension.js",
  "publisher": "fabiospampinato",
  "activationEvents": [
    "onCommand:diff.file"
  ],
  "contributes": {
    "commands": [
      {
        "command": "diff.file",
        "title": "Diff: File"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "publish": "vsce publish && npm publish",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/vscode-diff/issues"
  },
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/vscode-diff.git"
  },
  "engines": {
    "vscode": "^1.18.0"
  },
  "keywords": [
    "vscode",
    "vsc",
    "extension",
    "diff"
  ],
  "categories": [
    "Other"
  ],
  "dependencies": {
    "absolute": "0.0.1",
    "lodash": "^4.17.4",
    "pify": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^6.0.40",
    "tslint": "^4.4.2",
    "typescript": "~2.2.0",
    "vscode": "^1.1.4"
  }
}
