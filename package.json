{
  "name": "vscode-diff",
  "displayName": "Diff",
  "description": "Diff 2 opened files with ease. Because running `code --diff path1 path2` is too slow.",
  "icon": "resources/logo-128x128.png",
  "version": "1.3.6",
  "license": "MIT",
  "main": "out/extension.js",
  "publisher": "fabiospampinato",
  "activationEvents": [
    "onCommand:diff.file"
  ],
  "contributes": {
    "commands": [
      {
        "command": "diff.file",
        "title": "Diff: File"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "rm -rf out && webpack --mode production",
    "publish": "vsce publish && npm publish",
    "compile": "webpack --mode development",
    "compile:watch": "webpack --mode development --watch",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/vscode-diff/issues"
  },
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/vscode-diff.git"
  },
  "engines": {
    "vscode": "^1.18.0"
  },
  "keywords": [
    "vscode",
    "vsc",
    "extension",
    "diff"
  ],
  "categories": [
    "Other"
  ],
  "dependencies": {
    "@types/node": "^10.12.8",
    "absolute": "0.0.1",
    "lodash": "^4.17.4",
    "pify": "^3.0.0"
  },
  "devDependencies": {
    "ts-loader": "^5.2.1",
    "typescript": "~2.4.1",
    "vscode": "^1.1.4",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  }
}
